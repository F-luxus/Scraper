{% extends 'base.html.twig' %}

{% block title %}Hello TimeController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    {% if app.user %}
        <div class="mb-3 mygtukai">
        {% for Times in timeRow %}
        {{ Times.userid }}
        {% endfor %}
           Sveiki, {{ app.user.username }} ir tavo id yra {{ app.user.id }}[<a href="{{ path('app_logout') }}">Atsijungti</a>]<br>
		{{ form(form) }}
		   
        </div>
    {% endif %}
    {% if not app.user %}
          <div class="mb-3">
            Tu nepasijunges
        </div>    
    {% endif %}
{% endblock %}
